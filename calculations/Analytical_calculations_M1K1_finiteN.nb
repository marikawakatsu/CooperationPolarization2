  RowBox[{
   RowBox[{
    RowBox[{"sumRule", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{"expr1_", ",", "iter_"}], "]"}], "+", 
       RowBox[{"Sum", "[", 
        RowBox[{"expr2_", ",", "iter_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"expr1", "+", "expr2"}], ",", "iter"}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"prodRule", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{"expr1_", ",", "iter1_"}], "]"}], "*", 
       RowBox[{"Sum", "[", 
        RowBox[{"expr2_", ",", "iter2_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"expr1", "*", "expr2"}], ",", "iter1", ",", "iter2"}], 
       "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"revSumRule", "=", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"expr1_", "+", "expr2_"}], ",", "iter_"}], "]"}], 
      "\[RuleDelayed]", " ", 
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{"expr1", ",", "iter"}], "]"}], "+", 
       RowBox[{"Sum", "[", 
        RowBox[{"expr2", ",", "iter"}], "]"}]}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"constRule", "=", 
     RowBox[{
      RowBox[{"Sum", "[", 
